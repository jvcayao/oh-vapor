{
    "name": "douglasthwaites/oh-vapor",
    "description": "A package to allow Oh-Dear through the AWS WAF controlled by Vapor.",
    "type": "plugin",
    "autoload": {
        "psr-4": {
            "DouglasThwaites\\OhVapor\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Doug Thwaites",
            "email": "hello@dougthwaites.nz"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "aws/aws-sdk-php": "^3.346",
        "symfony/yaml": "^4.2|^5.0|^6.0",
        "laravel/prompts": "^0.3.5",
        "ohdearapp/ohdear-php-sdk": "^3.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DouglasThwaites\\OhVapor\\OhVaporServiceProvider"
            ]
        }
    }
}
